$(function() {

    $(".allownumericwithdecimal").on("keypress keyup blur",function (event) {
        //this.value = this.value.replace(/[^0-9\.]/g,'');
        $(this).val($(this).val().replace(/[^0-9\.]/g,''));
        if ((event.which != 46 || $(this).val().indexOf('.') != -1) && (event.which < 48 || event.which > 57)) {
            event.preventDefault();
        }
    });

    $(".caja-consulta > .btn-consulta").click(function(e){
        $(".costo_envio").text("");
        $(".costo_envio").hide();
        $(".error_calculo").hide()
        let montoVal = $("#monto").val().split('.').join("");
        $.post(
            APP_URL + '/ws_interno/calcularMonto',
            {   '_token': $token,
                //'monto': $("#monto").val()
                'monto': montoVal
            },
            function (data) {
                if(data.errores ==''){
                    $("#monto_final").val(separador(data.monto_final));
                    $(".costo_envio").html(" Para enviar <strong> $"+$("#monto").val()+" </strong> deber√°s pagar&nbsp;<strong>$"+$("#monto_final").val() + "*</strong> <br> <span>* Incluye monto a enviar + Cargo</span>");
                    $(".costo_envio").show();

                }
                else{
                    var error = "";
                    if(typeof data.errores ==='string')
                        error = data.errores;
                    else
                        error = data.errores.monto[0];
                    $(".error_calculo").text(error).show();
                }

            }
        )
        e.preventDefault();
        $(".error_calculo").text("");

    });

    function separador(valor) {
        valor = new Intl.NumberFormat('de-DE', {
            //style: 'currency',
            //currency: 'ARS'
        }).format(valor);
        return valor
    }

});
